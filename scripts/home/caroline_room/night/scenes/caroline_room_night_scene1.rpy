label caroline_room_night_scene1_label:
    hide screen week_day_viewer
    hide screen time_skip_button
    hide screen day_time_viewer
    hide screen map_button

    if caroline_room_can_night_scene1 == False:
        show screen caroline_room_night
        $ clickable = False
        MC "(Я уже сделал то что хотел. Пусть отдыхает.)"
        $ clickable = True
        jump caroline_room_morning1
    if caroline_room_can_night_scene1 == True:

        $ renpy.music.stop(channel="music2", fadeout=1)
        $ renpy.music.play('/sfx/Miami Viceroy.mp3', channel="music1", loop=True, fadein = 2)

        scene caroline_room_night_scene1_p1 with dissolve
        $ can_hide_windows = True
        MC "(Надеюсь, я не зашел слишком рано. Мне нужно подождать, пока Кэролайн крепко уснет.)"
        MC "(Хм ... Кажется, она не двигалась с тех пор, как я открыл дверь.)"
        MC "(Наверное, мне безопасно ползти по комнате.)"

        scene caroline_room_night_scene1_p2
        MC "(Посмотрите на эти груди! Они чертовски массивные!)"
        MC "(Хе-хе ... Я собираюсь повеселиться с тобой сегодня, Кэролайн.)"
        MC "(Но с чего начать…)"
        jump caroline_room_night_scene1_menu


label caroline_room_night_scene1_menu:
    window hide
    menu:
        "Разбудить ее." if Caroline_points >=2:
            jump CR2_NS_label
        "Вставить свой член в рот.":

            scene caroline_room_night_scene1_mounth_p1
            MC "(Хорошо, давай положим мой член в рот, пока она спит.)"
            MC "(Это будет рискованно. Губы довольно чувствительны, и я могу очень легко разбудить ее, если не буду осторожен.)"
            MC "(Тише едешь, дальше будешь…)"

            scene caroline_room_night_scene1_mounth_p2
            MC "ух…"
            MC "(Я чувствую теплое дыхание из ее носа.)"
            MC "(Еще полдюйма!)"
            MC "(Черт! Ее зубы мешают.)"

            scene caroline_room_night_scene1_mounth_p3
            MC "(Мне придется заставить ее дышать через рот.)"
            MC "(Посмотрим, работает ли это.)"
            MC "Извини Кэролайн…"

            scene caroline_room_night_scene1_mounth_p4
            MC "(Продолжим! Она начинает открывать рот!)"
            MC "(Я почти в..!)"
            Caroline "Ааххх…"

            scene caroline_room_night_scene1_mounth_p5
            MC "ААА…"
            scene caroline_room_night_scene1_mounth_p5anim
            $ renpy.pause()
            scene caroline_room_night_scene1_mounth_p5anim2
            $ renpy.pause()
            MC "(Это так хорошо!)"
            scene caroline_room_night_scene1_mounth_p5anim3
            $ renpy.pause()
            MC "(Вот и все, Кэролайн. Соси мой член!)"

            scene caroline_room_night_scene1_mounth_p6
            MC "(Боже ... Мне жаль, что я не могу вставить его глубже!)"
            MC "(Может быть, если я действительно буду осторожен, я выкрою себе еще пол дюйма.)"
            MC "(Черт возьми, я зашел так далеко! Давай сделаем это!)"

            scene caroline_room_night_scene1_mounth_p7
            MC "Aхх ... ебать…"
            MC "(Это так хорошо!)"
            Caroline "Ммм ... Хмм…"
            MC "(Вот дерьмо! Она начинает просыпаться!)"

            scene caroline_room_night_scene1_mounth_p8
            MC "(Трахать ебать ебать!)"
            MC "(Я должен вытащить свой член из ее рта и уйти из комнаты, прежде чем она поймает меня!)"

            scene black
            MC "(Дерьмо, это было ДЕЙСТВИТЕЛЬНО близко!)"
            $ renpy.music.stop(channel="music1", fadeout=1)
            $ renpy.pause(2, hard = True)
            $ renpy.sound.play("sfx/door_open.mp3")
            $ renpy.music.play('/sfx/Sock Hop.mp3', channel="music2", loop=True, fadein = 2)
            $ caroline_room_can_night_scene1 = False
            $ can_hide_windows = False
            jump corridor_morning1
        "Потереть свой член об ноги.":
            scene caroline_room_night_scene1_feet_p1
            MC "(Давай сыграем с твоими ногами немного.)"
            MC "(Кэролайн была достаточно любезна, чтобы не одевать носки в на ночь.)"
            MC "(Ммм, у тебя действительно великолепные ноги, Кэролайн!)"

            scene caroline_room_night_scene1_feet_p2
            MC "(Хорошо, время, чтобы твои пальцы поработали на мой член!)"
            MC "(Там мы идем-почти там…)"

            scene caroline_room_night_scene1_feet_p3
            MC "Аааа... "
            scene caroline_room_night_scene1_feet_p3anim
            MC "(Мне просто хотелось схватить обе ее ноги и засунуть между ними.)"
            MC "(Может быть, я мог бы воспользоваться и другой ногой, прежде чем она проснется?)"

            scene caroline_room_night_scene1_feet_p4
            MC "Медленно делаем это…"
            Caroline "Хм ... Ммм…"
            MC "(ДЕРЬМО! Я слишком сильно подтолкнул ее!)"
            MC "(Не просыпайся ... не просыпайся…)"

            scene caroline_room_night_scene1_feet_p5
            Caroline "Ммм... "
            Caroline "Yawwwwn!"
            MC "(О, о! Время выходить!)"
            scene black
            $ renpy.music.stop(channel="music1", fadeout=1)
            $ renpy.pause(2, hard = True)
            $ renpy.sound.play("sfx/door_open.mp3")
            $ renpy.music.play('/sfx/Sock Hop.mp3', channel="music2", loop=True, fadein = 2)
            $ caroline_room_can_night_scene1 = False
            $ can_hide_windows = False
            jump corridor_morning1
        "Стянуть ее лифчик и играть с сиськами.":
            scene caroline_room_night_scene1_tits_p1

            MC "(Она выглядит крепко спящей. Если я действительно буду осторожен, я, возможно, буду играть с ее сиськами.)"
            MC "(Ну, здесь ничего не получается…)"

            scene caroline_room_night_scene1_tits_p2
            MC "Вау…"
            MC "(Просто посмотрите на этот сосок. Я бы хотел ласкать его!)"
            MC "(И ее изгибы удивительны. Мой член прекрасно сочетается между ними!)"

            scene caroline_room_night_scene1_tits_p3
            MC "(Ммм! Так мягко и тепло!)"
            MC "(Не похоже, что я ее беспокою - может быть, мне удастся немного сжать их?)"
            MC "(Тут ничего не получиться!)"

            scene caroline_room_night_scene1_tits_p4
            MC "(Бог, ее сиськи безупречны.)"
            MC "(Интересно, как она реагирует, если она проснется прямо сейчас?)"
            scene caroline_room_night_scene1_tits_p4anim with dissolve

            MC "(Лучше не думать об этом…)"
            scene caroline_room_night_scene1_tits_p4 with dissolve
            MC "(Okay, just one final thing to do, then I’ll leave.)"

            scene caroline_room_night_scene1_tits_p5
            MC "Aх…."
            MC "(Я так трахаюсь прямо сейчас.)"
            MC "(Мне бы хотелось трахнуть ее грудь.)"

            scene caroline_room_night_scene1_tits_p6
            MC "ух…"
            Caroline "Хммм…."
            MC "(Ох о!)"

            scene caroline_room_night_scene1_tits_p7
            Caroline "Yawn!"
            MC "(Черт, она просыпается. Время идти!)"
            MC "(Это позор - все было просто хорошо!)"
            scene black
            $ renpy.music.stop(channel="music1", fadeout=1)
            $ renpy.pause(2, hard = True)
            $ renpy.sound.play("sfx/door_open.mp3")
            $ renpy.music.play('/sfx/Sock Hop.mp3', channel="music2", loop=True, fadein = 2)
            $ caroline_room_can_night_scene1 = False
            $ can_hide_windows = False
            jump corridor_morning1
        "Играть с ее задницей.":
            scene caroline_room_night_scene1_ass_p1
            MC "(Хорошо, Кэролайн. Давай посмотрим, чувствует ли твоя задница так же хорошо, как выглядит.)"
            MC "(У меня пальцы скрещены, что бы она достаточно глубоко спала, и не проснулась.)"

            scene caroline_room_night_scene1_ass_p2
            MC "(Все ее тело совершенное. Она выиграла бы джек-пот в генетической лотерее!)"
            if renpy.loadable("patch.rpy"):
                MC "(Они похожи с мамой. У них обоих блестящие тела!)"
            if not renpy.loadable("patch.rpy"):
                MC "(У них с Линдой есть схожесть. У них обоих блестящие тела!)"

            scene caroline_room_night_scene1_ass_p3
            Caroline "(храп…)"
            scene caroline_room_night_scene1_ass_p3anim
            MC "(Давайте начнем с массажа ее попки.)"
            MC "(Если она не проснется, то я увижу, как далеко я могу зайти.)"

            scene caroline_room_night_scene1_ass_p4
            MC "(Ммм, у нее удивительная задница!)"
            MC "(Хорошо, она, кажется, в достаточно глубоком сне.)"
            MC "(Начнем…)"

            scene caroline_room_night_scene1_ass_p5
            MC "Ммм…"
            MC "(Я бы с удовольствием трахнул ее в задницу в таком положении.)"
            MC "(Я думаю, у нее очень плотные булочки.)"

            scene caroline_room_night_scene1_ass_p6
            MC "(Я глубоко погрузил мой член в нее с длинными медленными движениями.)"
            MC "(Затем, когда я кончу, я подвину свои бедра вперед и кончил глубоко в ее задницу.)"
            MC "(Боже, эти мысли меня возбуждают!)"

            scene caroline_room_night_scene1_ass_p7
            MC "Оооо .... Я бы трахнул тебя так сильно…"
            Caroline "Хм?"
            MC "(Дерьмо! Я сказал это вслух?!)"

            scene caroline_room_night_scene1_ass_p8
            MC "(Время покидать корабль!)"
            if renpy.loadable("patch.rpy"):
                Caroline "(У меня был действительно странный сон ... Что-то о моем брате, трахающем мою задницу.)"
            if not renpy.loadable("patch.rpy"):
                Caroline "(У меня был действительно странный сон ... Что-то о моей подруге, трахающей мою задницу.)"
            Caroline "(Мне нужно перестать есть сыр так поздно ночью. Это дает мне странные мечты!)"
            scene black
            $ renpy.music.stop(channel="music1", fadeout=1)
            $ renpy.pause(2, hard = True)
            $ renpy.sound.play("sfx/door_open.mp3")
            $ renpy.music.play('/sfx/Sock Hop.mp3', channel="music2", loop=True, fadein = 2)
            $ caroline_room_can_night_scene1 = False
            $ can_hide_windows = False
            jump corridor_morning1


label CR2_NS_label:
    $ renpy.music.stop(channel="music2", fadeout=1)
    $ renpy.music.play('/sfx/March of the Spoons.mp3', channel="music1", loop=True, fadein = 2)
    if CR2_NS_firsttime == False:
        scene CR2_NS_p7 with dissolve
        $ can_hide_windows = True
        Caroline "[player_name]? Хочешь снова помогу с твоей большой проблемой??"
        Caroline "Что это будет, [player_name]?"
    else:


        scene CR2_NS_p1 with dissolve
        $ can_hide_windows = True
        MC "Привет, Кэролайн."
        Caroline "Хм? Который час?"
        Caroline "Я сплю?"

        scene CR2_NS_p2

        Caroline "хм?"
        Caroline "(Я забыла запереть дверь вчера вечером?)"
        Caroline "(Подожди ... Это еще темно..?)"

        scene CR2_NS_p3

        Caroline "[player_name]! Ты просто ворвался в мою комнату?!"
        MC "Нет, я-"
        Caroline "Что ты использовал? Отмычка, или что-то?"

        scene CR2_NS_p4

        MC "Н-нет! Я только что взял запасной ключ из магазина."
        Caroline "Ты украла мой запасной ключ?"
        MC "Я планировал дать его обратно..."

        scene CR2_NS_p5

        Caroline "Какого черта ты здесь?"
        MC "Я надеялся мы могли бы сделать что-нибудь вместе сегодня?"
        Caroline "Что ты имеешь в виду?"

        scene CR2_NS_p6

        MC "Мы могли бы сделать некоторые пикантные вещи вместе..?"
        MC "Никто не узнает пока мы будем молчать. Это ночь, в конце концов."
        Caroline "(Вздох) Прекрасно. Я думаю, я все еще должна тебе за фотографии."

        scene CR2_NS_p7

        Caroline "Что это будет, [player_name]?"
        Caroline "Ты хотел что бы я мастурбировала?"
        $ CR2_NS_firsttime = False

    $ renpy.music.stop(channel="music1", fadeout=1)
    $ renpy.music.play('/sfx/Miami Viceroy.mp3', channel="music2", loop=True, fadein = 2)
    window hide
    menu:
